{
  "name": "lambda-boilerplate",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "deploy": "npm run build && serverless deploy",
    "deploy:offline": "npm run build && serverless offline",
    "build": "ncc build src/index.ts",
    "docs": "typedoc",
    "pretest": "npm run build",
    "release": "semantic-release",
    "start": "cd test && serverless offline",
    "test": "npm run lint && npm run test:unit && npm run test:integration && pkg-ok",
    "test:integration": "concurrently --kill-others --success first \"cd test && serverless offline\" \"wait-on http://localhost:3000/dev/status && jest -c jest.integration.config.js\"",
    "test:unit": "jest",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "precommit:lint:fix": "tslint --project tsconfig.json --fix"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.395.0",
    "@middy/core": "^4.6.0",
    "@middy/event-normalizer": "^4.6.0",
    "@middy/http-cors": "^4.6.0",
    "@middy/http-error-handler": "^4.6.0",
    "@middy/http-json-body-parser": "^4.6.0",
    "@middy/http-security-headers": "^4.6.0",
    "@middy/sqs-partial-batch-failure": "^4.6.0",
    "@vercel/ncc": "^0.36.1",
    "husky": "^7.0.4",
    "joi": "^17.4.2",
    "mongodb": "^5.7.0",
    "reflect-metadata": "^0.1.13",
    "serverless-http": "^2.7.0",
    "serverless-offline": "^8.2.0",
    "serverless-vercel-ncc": "^0.0.7",
    "typedoc": "^0.22.8",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.85",
    "@types/jest": "^27.0.2",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "dotenv": "^16.3.1",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^27.3.1",
    "prettier": "^3.0.2",
    "serverless-deployment-bucket": "^1.5.1",
    "serverless-jest-plugin": "^0.4.0",
    "serverless-lift": "^1.27.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
